QLabel#lab_fieldstable_set
{
    image: url(:/images/theme_white/ok.svg);
    image-position: center;
    min-width: 16px;
    max-width: 16px;
    min-height: 16px;
    max-height: 16px;
}

QLabel#lab_fieldstable_unset
{
    image: url(:/images/theme_white/cancel.svg);
    image-position: center;
    min-width: 16px;
    max-width: 16px;
    min-height: 16px;
    max-height: 16px;
}

QLabel#lab_fieldstable_text
{
    color: rgb(70, 70, 70);
    background: white;
    border: none;
}



