{
    padding: 5px;
}

QTableWidget#table_pros
{
    color: rgb(70, 70, 70);
    background: white;
    border: none;
    selection-background-color: white;
}

QTableWidget#table_fields
{
    color: rgb(70, 70, 70);
    background: white;
    border: none;
    selection-background-color: white;
}

QScrollArea
{
    color: black;
    background: rgb(240,240,240);
    border: 0px;
}

